{
  "name": "react-rocket",
  "version": "0.1.0",
  "description": "React Starter Kit",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emrearabaci/react-rocket.git"
  },
  "keywords": [
    "react"
  ],
  "author": "a.emrearabaci@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emrearabaci/react-rocket/issues"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/config",
      "src/index.js"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "sass": "^1.49.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
